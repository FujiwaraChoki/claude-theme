  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#8A8A7A"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage",
        "storage.type"
      ],
      "settings": {
        "foreground": "#D97757"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#D97757"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#10b981"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#8b5cf6"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#f87171"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.class"
      ],
      "settings": {
        "foreground": "#a855f7"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "support.variable"
      ],
      "settings": {
        "foreground": "#C2C0B6"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant.language",
        "support.constant"
      ],
      "settings": {
        "foreground": "#8b5cf6"
      }
    },
    {
      "name": "Tags",
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#f87171"
      }
    },
    {
      "name": "Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#D97757"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation"
      ],
      "settings": {
        "foreground": "#D97757"
      }
    },
    {
      "name": "Properties",
      "scope": [
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#10b981"
      }
    },
    {
      "name": "Escape Characters",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#D97757"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#ef4444",
        "fontStyle": "underline"
      }
    }
  ]
}
